init main {
    let mat = array(
        array(1, 2, 3),
        array(4, 5, 6),
        array(7, 8, 9)
    )

    print(mat[1][2])     # â†’ 6
    mat[1][2] = 99
    print(mat)

    # while loop
    let x: int = 0
    while x < 3 {
        print(x)
        x = x + 1
    }

    # for loop
    for i in 0 .. 2 {
        for j in 0 .. 2 {
            print(mat[i][j])
        }
    }
}
